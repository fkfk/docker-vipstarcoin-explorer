{
  "network": "mainnet",
  "port": 3001,
  "services": [
    "vipstarcoin-api",
    "vipstarcoin-explorer",
    "vipstarcoind",
    "web"
  ],
  "servicesConfig": {
    "vipstarcoind": {
      "spawn": {
        "configdir": "/root/explorer",
        "datadir": "/root/explorer/data",
        "exec": "/usr/local/lib/node_modules/vipstarcoincore-node/bin/VIPSTARCOINd"
      }
    },
    "vipstarcoin-explorer": {
      "apiPrefix": "api",
      "routePrefix": "",
      "nodemapLink": ""
    },
    "vipstarcoin-api": {
      "routePrefix": "api",
      "db": {
        "host": "mongo",
        "port": "27017",
        "database": "vipstarcoin-explorer-mainnet",
        "user": "",
        "password": ""
      },
      "erc20": {
        "updateFromBlockHeight": 0
      }
    }
  }
}
